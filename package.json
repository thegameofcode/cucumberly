{
  "name": "cucuberly",
  "version": "0.0.1",
  "description": "Cucumber editor for Muggles",
  "main": "./src/start.js",
  "scripts": {
    "start": "node --harmony ./src/start.js",
    "jshint": "node --harmony node_modules/.bin/jshint ./src ./test ./features",
    "test": "node node_modules/.bin/mocha --harmony ./test/** ./test/**/**",
    "acceptance": "node --harmony node_modules/.bin/cucumber.js acceptance/features -r ./acceptance/lib/step_definitions --tags ~@pending --format pretty",
    "coverage": "node --harmony node_modules/.bin/istambul.js ./test/** ./features/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thegameofcode/cucumberly.git"
  },
  "author": "http://www.thegameofcode.com",
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.2.0",
    "cucumber": "^0.5.2",
    "istanbul": "^0.3.18",
    "jshint": "^2.8.0",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "mockery": "^1.4.0",
    "sinon": "^1.16.1"
  },
  "dependencies": {
    "async": "^1.4.2",
    "bunyan": "^1.4.0",
    "mongodb": "^2.0.42",
    "q": "^1.4.1",
    "request": "^2.61.0",
    "restify": "^4.0.0",
    "uuid": "^2.0.1"
  },
  "jshintConfig": {
    "esnext": true,
    "node": true,
    "unused": true
  }
}
